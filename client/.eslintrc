{
  "parserOptions": {
    "ecmaVersion": 2020
  },
  "extends": ["airbnb", "prettier"],
  "plugins": ["prettier"],
  "rules": {
    "react/prop-types": "off",
    "import/prefer-default-export": "off",
    "react/no-unescaped-entities": "off",
    "consistent-return": "off",
    "prettier/prettier": ["error"]
  }
}
